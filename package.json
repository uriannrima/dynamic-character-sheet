{
    "name": "dynamic-character-sheet-frontend",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "dev": "yarn install && run-p server:dev frontend:dev",
        "build": "yarn install && run-p server:host frontend:build",
        "host": "yarn install && run-p server:host frontend:host",
        "frontend:dev": "cd frontend && npm run dev",
        "frontend:build": "cd frontend && npm run build",
        "frontend:host": "cd frontend && npm run host",
        "server:dev": "cd server && npm run dev",
        "server:host": "cd server && npm run host",
        "server:lint": "cd server && npm run lint",
        "server:lint:fix": "cd server && npm run lint:fix"
    },
    "devDependencies": {
        "npm-run-all": "^4.0.2"
    },
    "dependencies": {}
}
