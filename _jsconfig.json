{
  "exclude": [
    "node_modules",
    "dist"
  ],
  "compilerOptions": {
    "target": "es2017",
    "allowSyntheticDefaultImports": false,
    "baseUrl": "./",
    "paths": {
      "@Feathers/*": [
        "src/feathers/*"
      ],
      "@Modules/*": [
        "src/modules/*"
      ],
      "@Sections/*": [
        "src/Sections/*"
      ],
      "@Services/*": [
        "src/services/*"
      ],
      "@Shared/*": [
        "src/shared/*"
      ],
      "@Store/*": [
        "src/store/*"
      ],
      "@Utils/*": [
        "src/utils/*"
      ]
    }
  }
}