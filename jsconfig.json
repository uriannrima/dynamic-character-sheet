{
  "compilerOptions": {
    // This must be specified if "paths" is set
    "baseUrl": ".",
    // Relative to "baseUrl"
    "paths": {
      "*": [
        "*",
        "src/*",
        "static/*",
        "src/services/*",
        "src/modules/*",
        "src/shared/*",
        "src/utilss/*"
      ],
      "Store/*":[
        "./src/store/*"
      ]
    }
  },
  "include": [
    "src/**/*"
  ]
}